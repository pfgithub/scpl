{
	"name": "scpl",
	"version": "1.14.0",
	"main": "built/index.js",
	"types": "built/index.d.ts",
	"license": "MIT",
	"homepage": "https://pfgithub.github.io/shortcutslang",
	"bugs": {
		"url": "https://github.com/pfgithub/shortcutslang"
	},
	"scripts": {
		"docs": "tsc && node ./built/src/GenDocs",
		"test": "jest --no-cache --coverage",
		"actions": "tsc && node ./built/src/Data/GenActions",
		"format": "yarn run prettier --write src/*** test/*.ts test/*.json index.ts .prettierrc .eslintrc.js modules.d.ts plistconv.js .travis.yml",
		"prepublishOnly": "yarn actions && yarn format && yarn docs && yarn test"
	},
	"author": {
		"name": "pfg",
		"email": "omegariisthebest@gmail.com",
		"url": "https://pfgithub.github.io/"
	},
	"repository": "github:pfgithub/shortcutslang",
	"files": [
		"/man",
		"/built/index.js",
		"/built/index.d.ts",
		"/built/src"
	],
	"jest": {
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json"
		],
		"testMatch": [
			"**/test/**/*.[jt]s?(x)",
			"**/?(*.)+(spec|test).[tj]s?(x)"
		],
		"testPathIgnorePatterns": [
			"/node_modules/",
			"/built"
		],
		"coverageReporters": [
			"json",
			"lcov",
			"clover"
		]
	},
	"dependencies": {
		"bplist-creator": "^0.0.7",
		"bplist-parser": "^0.1.1",
		"plist": "^3.0.1",
		"uuid": "^3.3.2"
	},
	"devDependencies": {
		"@types/jest": "^24.0.15",
		"@types/node": "^11.9.5",
		"@types/uuid": "^3.4.4",
		"@types/yargs": "^12.0.9",
		"@typescript-eslint/eslint-plugin": "^1.4.2",
		"@typescript-eslint/parser": "^1.4.2",
		"ava-ts": "^0.25.2",
		"codecov": "^3.6.5",
		"eslint": "^5.14.1",
		"jest": "^24.8.0",
		"prettier": "^1.16.4",
		"ts-jest": "^24.0.2",
		"ts-node": "^8.0.3",
		"typescript": "^3.3.3333",
		"webpack-cli": "^3.2.3"
	}
}
